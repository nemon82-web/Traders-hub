            color: #FFD700;
        }

        /* Cursor pointer for clickable world clock elements */
        .london-clock > div:first-child, .london-clock > div:nth-child(2),
        .newyork-clock > div:first-child, .newyork-clock > div:nth-child(2) {
            cursor: pointer;
        }

        .ascii-art_old {
